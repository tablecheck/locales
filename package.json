  "repository": {
    "type": "git",
    "url": "git+https://github.com/tablecheck/locales.git"
  },
  "bugs": {
    "url": "https://github.com/tablecheck/locales/issues"
  },
  "version": "2.4.1",
  "keywords": ["locales"],
