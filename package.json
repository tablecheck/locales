    "@auto-it/slack": "11.2.0",
    "@tablecheck/eslint-config": "11.0.0",
    "@tablecheck/prettier-config": "5.0.0",
    "@types/node": "22.13.13",
    "auditjs": "4.0.46",
    "auto": "11.3.0",
    "eslint": "8.57.1",
    "husky": "9.1.7",
    "prettier": "3.4.2",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vitest": "2.1.8"
